<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zqkc.dao.ILogsDao">
   
   <insert id="save" parameterType="Logs">
      insert into logger (id,uid,controller,action,params,consuming,status,createtime,isdel) values (#{id},#{uid},#{controller},#{action},#{params},#{consuming},#{status},#{createtime},#{isdel})
   </insert>
</mapper>